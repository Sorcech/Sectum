{
  "name": "sectum",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "version": "0.1.7",
  "description": "A modern Vue 3 component library built with UnoCSS, featuring beautiful UI components and layouts",
  "keywords": [
    "vue3",
    "unocss",
    "component-library",
    "ui",
    "typescript",
    "vite",
    "modern",
    "responsive",
    "accessibility"
  ],
  "homepage": "sectum.cc",
  "author": {
    "name": "Cesar.Studio"
  },
  "license": "MIT",
  "main": "./dist/sectum.umd.js",
  "module": "./dist/sectum.es.js",
  "exports": {
    ".": {
      "import": "./dist/sectum.es.js",
      "require": "./dist/sectum.umd.js"
    },
    "./dist/style.css": "./dist/sectum.css",
    "./dist/uno.config.ts": "./dist/uno.config.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "config": {
    "uno": "./dist/uno.config.ts"
  },
  "scripts": {
    "dev": "vite --port 3000 --open",
    "dev:http": "vite --host localhost --port 3000",
    "dev:https": "cross-env VITE_HTTPS=true vite --host localhost --port 3000",
    "dev:secure": "vite --https --host localhost --port 3000",
    "build": "rimraf dist && vite build",
    "build:lib": "rimraf dist && vite build --mode library",
    "build:site": "rimraf dist-site && vite build --mode production",
    "preview": "vite preview",
    "preview:site": "vite preview dist-site",
    "prepublishOnly": "npm run build:lib",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "_scripts_comments": {
    "dev": "启动开发服务器,端口 3000,自动打开浏览器",
    "dev:http": "启动开发服务器(HTTP),监听 localhost",
    "dev:https": "启动开发服务器(HTTPS),通过环境变量启用",
    "dev:secure": "启动开发服务器(HTTPS),直接启用 SSL",
    "build": "构建项目(默认模式,用于网页版本)",
    "build:lib": "构建组件库(用于 npm 发布)",
    "build:site": "构建网页版本(输出到 dist-site 目录,用于网站部署)",
    "preview": "预览构建后的文件(默认 dist 目录)",
    "preview:site": "预览构建后的网页版本(dist-site 目录)",
    "prepublishOnly": "npm publish 前自动执行的钩子,确保发布前构建组件库",
    "publish:patch": "发布补丁版本(0.1.7 -> 0.1.8)",
    "publish:minor": "发布次要版本(0.1.7 -> 0.2.0)",
    "publish:major": "发布主要版本(0.1.7 -> 1.0.0)"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "@unocss/preset-uno": "^66.5.0",
    "highlight.js": "^11.11.1",
    "unocss": "^66.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@mdit-vue/plugin-headers": "^3.0.2",
    "@mdit-vue/types": "^3.0.2",
    "@shikijs/transformers": "^3.14.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "async-validator": "^4.2.5",
    "cross-env": "^7.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-link-attributes": "^4.0.1",
    "rimraf": "^6.0.1",
    "shiki": "^3.14.0",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-markdown": "^29.1.0",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.2",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.0.6"
  }
}
